<!DOCTYPE html>

<html xmlns="http://www.w3.org/1999/xhtml"
	  xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
	  xmlns:p="tapestry:parameter">

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" />
    <meta name="apple-mobile-web-app-capable" content="yes" />
    	
    <title>${pageTitle}</title>
    
    <!-- Core CSS - Include with every page -->
    <link rel="stylesheet" href="${context:/css/bootstrap.min.css}" />
    <link rel="stylesheet" href="${context:/font-awesome/css/font-awesome.css}" />

    <!-- SB Admin CSS - Include with every page -->
    <link rel="stylesheet" href="${context:/css/sb-admin-2.css}" />
            
    <!-- Core Scripts - Include with every page -->
    <script src="${context:/js/jquery-1.11.0.js}" />
    <script src="${context:/js/bootstrap.min.js}" />

    <!-- SB Admin Scripts - Include with every page -->
    <script src="${context:/js/sb-admin-2.js}" />
    
</head>

<body>

    	<t:if test="userSession">

    		<div id="wrapper">
        		<nav class="navbar navbar-default navbar-fixed-top" role="navigation" style="margin-bottom: 0">
            		<div class="navbar-header">
                		<button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".sidebar-collapse">
                    		<span class="sr-only">Toggle navigation</span>
                    		<span class="icon-bar"></span>
                    		<span class="icon-bar"></span>
                    		<span class="icon-bar"></span>
                		</button>
                		<a class="navbar-brand" href="#">${message:header}</a>
            		</div>            		

    					<ul class="navbar-top-links navbar-right">

                			<li class="dropdown">
                    			<a href="#" class="dropdown-toggle" data-toggle="dropdown">
                        			<i class="fa fa-location-arrow fa-fw"></i>  
                        			<i class="fa fa-caret-down"></i>
                    			</a>
                    			<ul class="dropdown-menu dropdown-user">
                        			<li>                        				
                        				<i class="fa fa-location-arrow fa-fw"></i>                        				
                        			</li>                                                			
                    			</ul>
                    			
                    			<ul class="dropdown-menu dropdown-user">
                        			<li>
                        				<a href="#" t:type="PageLink" t:page="park/CreatePark">
                        					<i class="fa fa-user fa-fw"></i> ${message:menu-createPark}
                        				</a>
                        			</li>                        
                        			<li class="divider" />
                        			<li>
                        				<a href="#" t:type="PageLink" t:page="park/CreatePark">
                        					<i class="fa fa-user fa-fw"></i> ${message:menu-createPark}
                        				</a>
                        			</li>
                    			</ul>                    			                    			
                			</li>    					

                			<li class="dropdown">
                    			<a href="#" class="dropdown-toggle" data-toggle="dropdown">
                        			<i class="fa fa-users fa-fw"></i>  
                        			<i class="fa fa-caret-down"></i>
                    			</a>
                    			<ul class="dropdown-menu dropdown-user">
                        			<li>                        				
                        				<i class="fa fa-user fa-fw"></i>                        				
                        			</li>                                                			
                    			</ul>
                			</li>    					

    					
                			<li class="dropdown">
                    			<a href="#" class="dropdown-toggle" data-toggle="dropdown" >
                        			<i class="fa fa-calendar fa-fw"></i>  
                        			<i class="fa fa-caret-down"></i>
                    			</a>
                    			<ul class="dropdown-menu dropdown-messages" data-role="dropdown" data-effect='fade'>
                        			<li>
                            			<a href="#">
                                			<div>
                                    			<strong>Timetable</strong>
                                    			<span class="pull-right text-muted">
                                        			<em>Yesterday</em>
                                    			</span>
                                			</div>                                			
                            			</a>
                        			</li>
                        			<li class="divider"></li>                                                
                        			<li>
                            			<a class="text-center" href="#">
                                			<strong>View All Calendars</strong>
                                			<i class="fa fa-angle-right"></i>
                            			</a>
                        			</li>
                    			</ul>
                    	    </li>            		    					
    					    					
                			<li class="dropdown">
                    			<a href="#" class="dropdown-toggle" data-toggle="dropdown" >
                        			<i class="fa fa-envelope fa-fw"></i>  
                        			<i class="fa fa-caret-down"></i>
                    			</a>
                    			<ul class="dropdown-menu dropdown-messages" data-role="dropdown" data-effect='fade'>
                        			<li>
                            			<a href="#">
                                			<div>
                                    			<strong>John Smith</strong>
                                    			<span class="pull-right text-muted">
                                        			<em>Yesterday</em>
                                    			</span>
                                			</div>
                                			<div>Lorem ipsum dolor sit amet, consectetur adipiscing elit. Pellentesque eleifend...</div>
                            			</a>
                        			</li>
                        			<li class="divider"></li>                                                
                        			<li>
                            			<a class="text-center" href="#">
                                			<strong>Read All Messages</strong>
                                			<i class="fa fa-angle-right"></i>
                            			</a>
                        			</li>
                    			</ul>
                    	    </li>

                			<li class="dropdown">
                    			<a href="#" class="dropdown-toggle" data-toggle="dropdown">
                        			<i class="fa fa-bell fa-fw"></i>  
                        			<i class="fa fa-caret-down"></i>
                    			</a>
                    			<ul class="dropdown-menu dropdown-alerts" data-role="dropdown" data-effect='fade'>
                        			<li>
                            			<a href="#">
                                			<div>
                                    			<i class="fa fa-comment fa-fw"></i> New Comment
                                    			<span class="pull-right text-muted small">4 minutes ago</span>
                                			</div>
                            			</a>
                        			</li>
                        			<li class="divider"></li>
                        			<li>
                            			<a href="#" class="text-center">
                                			<strong>See All Alerts</strong>
                                			<i class="fa fa-angle-right"></i>
                            			</a>
                        			</li>
                    			</ul>
                			</li>
                    	    
                			<li class="dropdown">
                    			<a href="#" class="dropdown-toggle" data-toggle="dropdown" >
                        			<i class="fa fa-tasks fa-fw"></i>  
                        			<i class="fa fa-caret-down"></i>
                    			</a>
                    			<ul class="dropdown-menu dropdown-tasks">
                        			<li>
                            			<a href="#">
                                			<div>
                                    			<p>
                                        			<strong>Task 1</strong>
                                        			<span class="pull-right text-muted">40% Complete</span>
                                    			</p>
                                    			<div class="progress progress-striped active">
                                        			<div class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="40" aria-valuemin="0" aria-valuemax="100" style="width: 40%">
                                            			<span class="sr-only">40% Complete (success)</span>
                                        			</div>
                                    			</div>
                                			</div>
                            			</a>
                        			</li>
                        			<li class="divider"></li>
                        			<li>
                            			<a class="text-center" href="#">
                                			<strong>See All Tasks</strong>
                                			<i class="fa fa-angle-right"></i>
                            			</a>
                        			</li>
                    			</ul>
                			</li>                    	    
                    	        					
                			<li class="dropdown">
                    			<a href="#" class="dropdown-toggle" data-toggle="dropdown">
                        			<i class="fa fa-user fa-fw"></i>  
                        			<i class="fa fa-caret-down"></i>
                    			</a>
                    			<ul class="dropdown-menu dropdown-user">
                        			<li>
                        				<a href="#" t:type="PageLink" t:page="user/UpdateProfile">
                        					<i class="fa fa-user fa-fw"></i> ${message:menu-updateProfile}
                        				</a>
                        			</li>                        
                        			<li class="divider" />
                        			<li>
                        				<a href="#" t:type="ActionLink" t:id="logout">
                        					<i class="fa fa-sign-out fa-fw"></i> ${message:menu-logout}
                        				</a>
                        			</li>
                    			</ul>
                			</li>    					
						</ul>
           		</nav>
        	</div>


        <!-- /#wrapper -->
         <p:else>
            <ul>
	          <li>
  	            <a href="#" t:type="PageLink" t:page="user/Login">${message:menu-authenticate}</a>
	          </li>
	        </ul>
          </p:else>
          </t:if>    

<!-- Content. -->

  	<t:body/>



<!-- End of HTML body. -->
</body>

</html>